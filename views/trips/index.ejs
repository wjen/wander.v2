<!DOCTYPE html>
<html>
<head>
<link href='https://fonts.googleapis.com/css?family=Roboto:900&subset=latin,latin-ext' rel='stylesheet' type='text/css'>
<link rel="stylesheet" href="/css/main.css">
 <%- include('../partials/head') %>
</head>
<body>
 <%- include('../partials/navbar') %>
 <%- include('../partials/header', {subHeading: 'TRIPS'}) %>
<main>
<div class="container-fluid-trips">
  <% tripData.forEach(function(trip) { %>
    <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
      <div class='trip'>
        <a href="/trips/<%= trip[2] %>">
          <img src="<%= trip[1] %>" alt="">
        </a>
        <div class='title'>
          <%= trip[0] %>
        </div>
      </div>
    </div>
  <% }); %>
</div>


</main>
 <%- include('../partials/footer') %>
 <%- include('../partials/scripts') %>
</body>
</html>
